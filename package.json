{
  "publishConfig": {
    "tag": "next"
  },
  "name": "bootprint-openapi",
  "version": "1.0.1",
  "description": "Converts a openapi-definition into a static html page",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "mocha --recursive && npm run lint",
    "coverage": "rm coverage -rf && istanbul cover --dir coverage/mocha ./node_modules/.bin/_mocha --print none -- --recursive",
    "postcoverage": "istanbul report --root coverage text-summary html  && istanbul check-coverage --statements 100",
    "thought": "thought run -a",
    "version": "thoughtful changelog -o -a && npm run thought"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bootprint/bootprint-openapi"
  },
  "author": "Nils Knappmeier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bootprint/bootprint-openapi/issues"
  },
  "homepage": "https://github.com/bootprint/bootprint-openapi",
  "dependencies": {
    "bootprint-json-schema": "^2.0.0-rc.3",
    "highlight.js": "^9.12.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.4",
    "m-io": "^0.5.0"
  },
  "devDependencies": {
    "bootprint-unit-testing": "^2.1.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "thought": "^1.5.1",
    "thought-plugin-bootprint": "^1.1.0",
    "thoughtful-release": "^0.3.1",
    "trace-and-clarify-if-possible": "^1.0.0"
  },
  "peerDependencies": {
    "bootprint": "^2.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "openapi-to-html",
    "swagger-to-html",
    "swagger"
  ],
  "files": [
    "handlebars",
    "less",
    "index.js",
    "lib"
  ]
}
